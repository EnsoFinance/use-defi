{
  "name": "@ensofinance/use-defi",
  "version": "0.1.15",
  "description": "React hooks library to use with enso shortcuts infrastructure",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm generate",
    "lint": "pnpm eslint src/*",
    "test": "pnpm vitest run --config ./test/vitest.config.ts",
    "generate": "npx openapi-typescript https://api.enso.finance/api-json -o src/generated/api.d.ts",
    "build": "pnpm generate && pnpm build:setup && rollup -c",
    "build:setup": "pnpm gts clean && mkdir dist && jq 'del(.scripts) | del(.devDependencies) | del(.private) | del(.pnpm)' package.json > dist/package.json",
    "dev": "cd playground && vite dev"
  },
  "keywords": [],
  "author": "@ensofinance",
  "license": "ISC",
  "peerDependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@wagmi/core": "^1.3.7",
    "react": "^18.2.0",
    "react-query": "^3.39.3",
    "wagmi": "^1.3.9"
  },
  "dependencies": {
    "query-string": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.4.4",
    "vitest": "^0.33.0",
    "@rollup/plugin-commonjs": "^25.0.2",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@testing-library/react": "^14.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-react": "^4.0.1",
    "esbuild": "^0.18.16",
    "eslint": "^8.44.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "fast-glob": "^3.3.0",
    "gray-matter": "^4.0.3",
    "gts": "^3.1.1",
    "jsdom": "^22.1.0",
    "openapi-typescript": "^6.3.7",
    "prettier": "^3.0.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.26.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-includepaths": "^0.2.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "viem": "^1.2.15",
    "vite": "^4.4.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}